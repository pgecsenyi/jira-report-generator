{
    "editor.formatOnSave": true,
    "editor.rulers": [
        120
    ],
    "editor.tabSize": 4,
    "files.insertFinalNewline": true,
    "python.autoComplete.extraPaths": [
        "${workspaceRoot}"
    ],
    "python.formatting.autopep8Path": "~/.virtualenvs/src-00000000/bin/autopep8",
    "python.formatting.provider": "autopep8",
    "python.linting.pep8Args": [
        "--max-line-length=120"
    ],
    "python.linting.pylintArgs": [
        "--rcfile=${workspaceRoot}/.pylintrc"
    ],
    "python.linting.pylintPath": "~/.virtualenvs/src-00000000/bin/pylint",
    "python.pythonPath": "~/.virtualenvs/src-00000000/bin/python",
    "python.venvPath": "~/.virtualenvs"
}
